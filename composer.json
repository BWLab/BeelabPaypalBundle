{
    "name": "beelab/paypal-bundle",
    "type": "symfony-bundle",
    "description": "Simple bundle for Paypal integration",
    "keywords": ["Paypal", "payment"],
    "homepage": "https://bee-lab.net",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Massimiliano Arione ",
            "email": "massimiliano.arione@bee-lab.it"
        }
    ],
    "require": {
        "php": ">=5.3.8",
        "symfony/symfony": "~2.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "omnipay/paypal": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Beelab\\PaypalBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
